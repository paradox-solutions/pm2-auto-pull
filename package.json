{
  "name": "pm2-auto-pull",
  "version": "2.00.0",
  "description": "PM2 module to auto pull applications when there is an update",
  "main": "app",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "debug": "*",
    "lodash": "^4.17.10",
    "pm2": "*",
    "pmx": "latest",
    "request": "^2.85.0",
    "request-promise": "^4.2.2"
  },
  "config": {
    "interval": 30000,
    "notify": true,
    "notify_path": "/pm2/notify",
    "notify_env_port": "CHECK_PORT",
    "notify_port": false,
    "notify_timeout": 3000
  },
  "apps": [
    {
      "script": "app.js",
      "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
      "merge_logs": true
    }
  ],
  "author": "",
  "license": "ISC"
}
